    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BE\BE.csproj" />
    <ProjectReference Include="..\BLL\BLL.csproj" />
    <ProjectReference Include="..\INFRA\INFRA.csproj" />
  </ItemGroup>

</Project>